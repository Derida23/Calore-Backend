{
  "name": "calore-backend",
  "version": "1.0.0",
  "description": "cashier system for calore food truck",
  "main": "app.js",
  "author": "arian derida hamami",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node --presets=@babel/preset-env app.js",
    "db:drop": "npx sequelize-cli db:drop",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "stop": "sudo kill -9 $(sudo lsof -t -i:4000)"
  },
  "keywords": [
    "heroku"
  ],
  "devDependencies": {
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.2.0"
  },
  "dependencies": {
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.14.9",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.29.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "mysql2": "^2.2.5",
    "path": "^0.12.7",
    "sequelize": "^6.6.5",
    "streamifier": "^0.1.1",
    "uuid": "^8.3.2"
  },
  "engines": {
    "yarn": "1.22.17",
    "node": "14.7.3"
  }
}
